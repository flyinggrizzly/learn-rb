      <section class="summary-information">
        <div class="row">
          <div class="small-20 small-offset-2 large-24 large-offset-0 column">
            <h1>Listing Labels</h1>
            <p id="notice"><%= notice %></p>
          </div>
        </div>
      </section>
      <section>
        <div class="row">
          <div class="small-20 small-offset-2 large-24 large-offset-0 column">
            <table class="content-items">
              <thead>
                <tr>
                  <th scope="col">Name</th>
                  <th>Description</th>
                </tr>
              </thead>
              <tbody id="labels">
                <% @labels.each do |label| %>
                  <tr>
                    <td><%= link_to label.name, edit_label_path(label) %></td>
                    <td><%= link_to label.description, edit_label_path(label) %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
            <%= link_to 'New Label', new_label_path, class: 'button-new' %>
          </div>
        </div>
      </section>
