      <section class="summary-information">
        <div class="row">
          <div class="small-20 small-offset-2 large-24 large-offset-0 column">
            <h1>Listing Users</h1>
            <p id="notice"><%= notice %></p>
          </div>
        </div>
      </section>
      <section>
        <div class="row">
          <div class="small-20 small-offset-2 large-24 large-offset-0 column">
            <table class="content-items">
              <thead>
                <tr>
                  <th scope="col">Name</th>
                </tr>
              </thead>
              <tbody id="users">
                <% @cms_users.each do |user| %>
                  <tr>
                    <td><%= link_to user.display_name, edit_user_path(user) %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>

            <%= link_to 'New User', new_user_path, class: 'button-new' %>
          </div>
        </div>
      </section>
